extends layout

include _includes/table

block content
  link(rel='stylesheet', href='../css/caag.css')
  table.table.table-responsive.caag
    thead.thead-inverse
      th Time
      th Fort Peck
      th Grand ABC
      th Lake Sammamish
      th Lake Couer d'Alene
      th Lake Chelan
      th Lake WA
    tbody
      tr
        td 8:30-9:30
        for event in events
         +displayEvent(event, "Fort Peck", '1487349000000')
        td
        for event in events
          +displayEvent(event, 'Lake Sammamish', '1487349000000')
        for event in events
          +displayEvent(event, "Lake Couer d'Alene", '1487349000000')
        td
        for event in events
          +displayEvent(event, "Lake Washington A", "1487349000000")
      tr
        td 9:50-10:50
        for event in events
          +displayEvent(event, "Fort Peck", "1487353800000")
        for event in events
          +displayEvent(event, "Grand Ballroom ABC", "1487353800000")
        td
        for event in events
          +displayEvent(event, "Lake Couer d'Alene", "1487353800000")
        for event in events
          +displayEvent(event, "Lake Chelan", "1487353800000")
        for event in events
          +displayEvent(event, "Lake Washington A", "1487353800000")
      tr
        td 11:00-12:00
        td.big-event(colspan=7) Grand Exhibit Opening
      tr
        td 12:15-1:15
        for event in events
          +displayEvent(event, "Fort Peck", "1487362500000")
        for event in events
          +displayEvent(event, "Grand Ballroom ABC", "1487362500000")
        for event in events
          +displayEvent(event, "Lake Sammamish", "1487362500000")
        for event in events
          +displayEvent(event, "Lake Couer d'Alene", "1487362500000")
        td
        for event in events
          +displayEvent(event, "Lake Washington A", "1487362500000")
      tr
        td 1:30-2:50
        td.big-event(colspan=7) General Session and Keynote
          p Featuring: Eph Ehly
          p With special guest Denese Odegaard and a performance by St. Patrick School's Choir
      tr
        td 3:00-3:30
        td.big-event(colspan=7) Visit the Exhibits
      tr
        td 3:30-4:30
        for event in events
          +displayEvent(event, "Fort Peck", "1487374200000")
        for event in events
          +displayEvent(event, "Grand Ballroom ABC", "1487374200000")
        for event in events
          +displayEvent(event, "Lake Sammamish", "1487374200000")
        for event in events
          +displayEvent(event, "Lake Couer d'Alene", "1487374200000")
        for event in events
          +displayEvent(event, "Lake Chelan", "1487374200000")
        for event in events
          +displayEvent(event, "Lake Washington A", "1487374200000")
      tr
        td 5:30-6:30
        td.big-event(colspan=7) WMEA Collegiate/Tri-M Dinner Evergreen North Foyer
      tr
        td 5:30-8:00
        td.big-event(colspan=7)
          a(href="list?findkey=category&findvalue=University%20Reception") University Receptions - see page 32 for details
          p.big-event A Very Jazzy Happy Hour: Sponsore by WMEA, Ted Brown Music and Quavermusic.com
      tr
        td 8:00-10:00
        td.big-event(colspan=7) Jazz Performance @ Hyatt Grand Ballroom
          p.big-event Featuring: Gail Pettis and Just 4 Kicks