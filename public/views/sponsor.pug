extends layout

block content
  .row.page-header
    .col-lg-12
      h1= pageHeader.title
      .error= message
  .row
    .col-xs-12.col-md-12
      .row
        .col-md-3
        .col-xs-12.col-md-6
          h3.exhibit= sponsor.sponsor
          p.exhibit_centered= sponsor.institution
          a.btn.btn-default(href='/sponsorUpdate/' + sponsor._id role='button') update
          a.btn.btn-default(href='/sponsorDelete/' + sponsor._id role='button') delete